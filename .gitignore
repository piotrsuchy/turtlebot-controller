# .gitignore for ROS2 projects

# ignore build and install directories
/build/
/install/

# .gitignore for nested log build and install directories
**/log/
**/build/
**/install/


# ignore log files and directories
src/log
log/
*.log

# Ignore autogenerated files
*.pyc
*.pyo
*.pyd
__pycache__/
*.so
*.swp
*.swo
*.swn
*.bak
*.tmp
.vscode/
